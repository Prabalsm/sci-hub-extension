{
    "name": "Sci-dl",
  "description": "Extension to make download easier",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
      "service_worker":"background.js"
  },
  "permissions":["storage","tabs"],
  "host_permissions": [
    "http://localhost:5000/*"],
  "action":{
    "default_icon": "icon.png",
    "default_popup":"popup.html"   
  }
}